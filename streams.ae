// ** map-reduce **

// dot product

1) var prod = a, b -> '* => '+

2) var prod = mapreduce((a, b), '*, '+)

3) var prod = 0
   for i in seq(len(a)) (
       prod += a[i] * b[i]
   )





// count total number of elements in each list
var ll // list of lists

1) var sum = ll -> len => '+

2) var sum = mapreduce(ll, len, '+)

3) var sum = 0
   for l in ll (
       sum += len(l)
   )
