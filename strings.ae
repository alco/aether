import strings

func compound_string_op(str) {
    str
    .tolower
    .replace("abc", "bcd")
    .toupper
    .reverse()
}

func compound_string_op2(str) {
    str
    -> .tolower
    -> .replace("abc", "bcd")
    -> .toupper
    -> .reverse
    ;
}

func bytes_to_string(bytes) {
    var buf = copy(bytes)
    var str = string(buf)

    buf[0] = 1 # compile error

    str
}
